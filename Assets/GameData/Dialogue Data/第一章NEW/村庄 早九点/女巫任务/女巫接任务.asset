    options: []
  - ID: "\u5973\u5DEB3"
    imageL: {fileID: 21300000, guid: c387867b8973a7b4b8ce77f5a9b3078b, type: 3}
    imageR: {fileID: 0}
    Name: "\u5965\u5C14\u745F\u96C5"
    END: 0
    num: 0
    text: "\u5618\u3002\u4E0D\u8981\u8BF4\u51FA\u6765\u3002\u770B\u6765\u8FD9\u91CC\u6BD4\u6211\u9884\u60F3\u5F97\u8FD8\u8981\u5371\u9669\uFF0C\u8981\u5FEB\u4E00\u70B9\u3002\u6211\u8981\u53BB\u505A\u4E00\u4E9B\u51C6\u5907\uFF0C\u4F60\u5462\uFF1F\u53E6\u4E00\u4E2A\u201C\u6211\u201D\u60F3\u8BA9\u4F60\u5E2E\u6211\u505A\u4EC0\u4E48\uFF1F"
    quest: {fileID: 0}
    item: {fileID: 0}
    options: []
  - ID: "\u4E3B\u89D22"
