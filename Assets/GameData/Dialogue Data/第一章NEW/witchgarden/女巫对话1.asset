    item: {fileID: 0}
    options: []
  - ID: "\u5973\u5DEB"
    imageL: {fileID: 21300000, guid: c387867b8973a7b4b8ce77f5a9b3078b, type: 3}
    imageR: {fileID: 0}
    Name: "\u5965\u5C14\u745F\u96C5"
    END: 0
    num: 0
    text: "\u540E\u5C71\u7684\u796D\u575B\u662F\u6211\u7684\u624B\u7B14\uFF0C\u53EF\u4EE5\u5229\u7528\u5B83\u5F71\u54CD\u8FD9\u7BC7\u533A\u57DF\u5185\u7684\u65F6\u95F4\uFF0C\u5C31\u50CF\u4F60\u521A\u521A\u4F7F\u7528\u4F60\u7684\u7434\uFF0C\u4ECE\u767D\u65E5\u7684\u540E\u5C71\u6765\u5230\u665A\u4E0A\u7684\u540E\u5C71\u4E00\u6837\u3002\u6211\u7565\u61C2\u4E00\u70B9\u8FD9\u672C\u4E66\u4E2D\u8BB0\u5F55\u7684\u7B26\u6587\uFF0C\u4F60\u9700\u8981\u5417\uFF1F"
    quest: {fileID: 0}
