  dialoguePieces:
  - ID: "\u987E\u5BA21"
    image: {fileID: 0}
    text: "\u751F\u9762\u5B54\uFF1F\u4F60\u4E00\u4E2A\u5916\u4E61\u4EBA\u6765\u8FD9\u513F\u5E72\u4EC0\u4E48\uFF0C\u4F60\u6700\u597D\u662F\u771F\u6709\u4E8B\u3002"
    quest: {fileID: 0}
    options:
    - text: "\u4F60\u6709\u6CA1\u6709\u542C\u8BF4\u8FC7\u4E00\u4E9B\u5947\u602A\u7684\u4F20\u95FB\uFF1F\u6BD4\u5982\u5973\u5DEB\u4E4B\u7C7B\u7684\uFF1F"
      targetID: "\u987E\u5BA22"
      takeQuest: 0
    - text: "\u90A3\u6CA1\u4E8B\u4E86\uFF0C\u6253\u6270\u4E86\uFF08\u79BB\u5F00\uFF09"
      targetID: 
      takeQuest: 0
  - ID: "\u987E\u5BA22"
    image: {fileID: 0}
    text: "\u6211\u597D\u50CF\u8FD8\u771F\u7684\u77E5\u9053\u4E9B\u4EC0\u4E48\uFF0C\u4F46\u6211\u6700\u8BA8\u538C\u4E0D\u4E0A\u9053\u7684\u4EBA"
    quest: {fileID: 0}
    options:
    - text: "\u8FD9\u4E2A\u53EF\u4EE5\u8BA9\u4F60\u5F00\u53E3\u4E86\u5417\uFF08\u4EA4\u51FA\u9053\u5177\u91D1\u5E01\uFF09"
      targetID: "\u987E\u5BA23"
      takeQuest: 0
    - text: "\u4F60\u6700\u597D\u653E\u5BA2\u6C14\u70B9\uFF0C\u6211\u662F\u6559\u4F1A\u6D3E\u6765\u8FFD\u67E5\u6B64\u4E8B\u7684"
      targetID: "\u987E\u5BA25"
      takeQuest: 0
    - text: "\u6211\u6CA1\u6709\u94B1"
      targetID: "\u987E\u5BA26"
      takeQuest: 0
    - text: "\u4F60\u8FD9\u662F\u4EC0\u4E48\u610F\u601D\uFF1F"
      targetID: "\u987E\u5BA27"
      takeQuest: 0
  - ID: "\u987E\u5BA23"
    image: {fileID: 0}
    text: "\u597D\u5427\uFF0C\u6211\u4EEC\u6751\u5B50\u91CC\u786E\u5B9E\u6709\u5973\u5DEB\uFF0C\u4F60\u770B\uFF0C\u5916\u9762\u6709\u6559\u4F1A\u7684\u4EBA\u5929\u5929\u67E5\u95EE\uFF0C\u5979\u5C31\u85CF\u5728\u6211\u4EEC\u4E4B\u4E2D\u3002\u5916\u4E61\u4EBA\uFF0C\u64E6\u4EAE\u4F60\u7684\u773C\u775B\u5427\u3002"
    quest: {fileID: 0}
    options:
    - text: "\u8FD9\u7B97\u4EC0\u4E48\u6D88\u606F..."
      targetID: "\u987E\u5BA24"
      takeQuest: 0
    - text: "...\u7136\u540E\uFF1F\uFF08\u4F60\u4F3C\u4E4E\u77E5\u9053\u81EA\u5DF1\u88AB\u9A97\u4E86\uFF09"
      targetID: 
